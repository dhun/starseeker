CREATE TABLE star_data (
    hip_num                     INTEGER not null    -- HIP番号
  , right_ascension             REAL    not null    -- 赤径
  , declination                 REAL    not null    -- 赤緯
  , magnitude                   REAL    not null    -- 等級
  , name                        TEXT                -- 通称
  , memo                        TEXT                -- 備考
  , constraint PK_fk_data primary key ( hip_num )
);
